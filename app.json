{
  "expo": {
    "name": "Snakelike",
    "slug": "snakelike",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "snakelike",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0b0f1a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0b0f1a"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    }
  }
}
